{
  "expo": {
    "name": "Simple Login Template",
    "slug": "mHealth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/health.png",
    "splash": {
      "image": "./assets/health.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/health.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.meant.mHealth"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "59917d86-484f-40a9-8681-23f09e119f10"
      }
    },
    "plugins": [
      [
        "expo-background-fetch",
        {
          "startOnBoot": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "permissions": [
              "INTERNET",
              "ACCESS_NETWORK_STATE"
            ],
            "useCleartextTraffic": true,
            "networkSecurityConfig": {
              "cleartextTrafficPermitted": true,
              "certificates": [
                {
                  "source": "system",
                  "overridePins": true
                },
                {
                  "source": "user",
                  "overridePins": true
                }
              ]
            }
          }
        }
      ]
    ],
    "owner": "meant"
  }
}